1. INSERT INTO customers (customerNumber, customerName, contactLastName, contactFirstName, phone, addressLine1, city, country, creditLimit) VALUES (5001, 'Pranay Traders', 'Reddy', 'Pranay', '9876543210', 'Madhapur', 'Hyderabad', 'India', 60000);
2. UPDATE customers SET creditLimit = creditLimit * 1.10 WHERE country = 'Germany';
3. DELETE FROM orders WHERE orderDate < '2005-01-01';
4. INSERT INTO products (productCode, productName, productLine, productScale, productVendor, productDescription, quantityInStock, buyPrice, MSRP) VALUES ('S99_9999', 'Vintage Car Model', 'Classic Cars', '1:18', 'Auto Models Ltd.', 'High-quality car model', 100, 50.00, 95.00);
5. INSERT INTO payments (customerNumber, checkNumber, paymentDate, amount) VALUES (103, 'CHK5001', '2025-01-01', 1500);
6. UPDATE offices SET phone = '+44 20-9999-8888' WHERE city = 'London';
7. DELETE FROM products WHERE productCode NOT IN (SELECT productCode FROM orderdetails);
8. INSERT INTO employees (employeeNumber, lastName, firstName, extension, email, officeCode, reportsTo, jobTitle) VALUES (2000, 'Sharma', 'Rohan', 'x5001', 'rohan.sharma@classic.com', '1', 1002, 'Sales Rep');
9. SELECT * FROM customers WHERE creditLimit > 50000;
10. SELECT * FROM employees WHERE officeCode = (SELECT officeCode FROM offices WHERE city = 'San Francisco');
11. SELECT * FROM orders WHERE status='Shipped' AND YEAR(orderDate)=2004;
12. SELECT * FROM products WHERE buyPrice BETWEEN 50 AND 100;
13. SELECT * FROM employees WHERE reportsTo = 1143;
14. SELECT * FROM customers WHERE city LIKE 'S%';
15. SELECT * FROM orders WHERE comments LIKE '%urgent%';
16. SELECT * FROM payments WHERE YEAR(paymentDate)=2003;
17. SELECT customerNumber, SUM(amount) FROM payments GROUP BY customerNumber;
18. SELECT customerNumber, COUNT(*) FROM payments GROUP BY customerNumber HAVING COUNT(*)>3;
19. SELECT officeCode, COUNT(*) FROM employees GROUP BY officeCode;
20. SELECT productLine, AVG(buyPrice) FROM products GROUP BY productLine HAVING AVG(buyPrice)>100;
21. SELECT c.customerNumber, SUM(od.quantityOrdered*od.priceEach) FROM customers c JOIN orders o USING(customerNumber) JOIN orderdetails od USING(orderNumber) GROUP BY c.customerNumber HAVING SUM(od.quantityOrdered*od.priceEach)>50000;
22. SELECT productCode, SUM(quantityOrdered) FROM orderdetails GROUP BY productCode;
23. SELECT status, COUNT(*) FROM orders GROUP BY status;
24. SELECT reportsTo, COUNT(*) FROM employees GROUP BY reportsTo HAVING COUNT(*)>2;
25. SELECT * FROM customers WHERE country='France' ORDER BY creditLimit DESC;
26. SELECT * FROM products ORDER BY quantityInStock ASC;
27. SELECT * FROM payments ORDER BY paymentDate DESC;
28. SELECT * FROM employees ORDER BY jobTitle, lastName;
29. SELECT * FROM customers ORDER BY creditLimit DESC LIMIT 5;
30. SELECT * FROM productlines ORDER BY productLine;
31. SELECT * FROM orders ORDER BY orderDate ASC LIMIT 10;
32. SELECT * FROM products ORDER BY productLine, buyPrice;
33. SELECT o.orderNumber, c.customerName, o.orderDate, o.status FROM orders o JOIN customers c USING(customerNumber);
34. SELECT e.employeeNumber, e.firstName, e.lastName, m.firstName, m.lastName FROM employees e LEFT JOIN employees m ON e.reportsTo=m.employeeNumber;
35. SELECT p.productName, od.quantityOrdered FROM products p JOIN orderdetails od USING(productCode);
36. SELECT c.* FROM customers c LEFT JOIN orders o USING(customerNumber) WHERE o.orderNumber IS NULL;
37. SELECT o.city, e.firstName, e.lastName FROM offices o LEFT JOIN employees e USING(officeCode);
38. SELECT o.orderNumber, p.productName, od.quantityOrdered, od.priceEach FROM orders o JOIN orderdetails od USING(orderNumber) JOIN products p USING(productCode);
39. SELECT c.customerName, e.firstName, e.lastName FROM customers c LEFT JOIN employees e ON c.salesRepEmployeeNumber=e.employeeNumber;
40. SELECT o.city, e.firstName, e.lastName FROM offices o LEFT JOIN employees e USING(officeCode);
41. SELECT * FROM orders WHERE shippedDate IS NULL;
42. SELECT DISTINCT pay.customerNumber, pay.amount, pr.productLine FROM payments pay JOIN customers c USING(customerNumber) JOIN orders o USING(customerNumber) JOIN orderdetails od USING(orderNumber) JOIN products pr USING(productCode);
43. SELECT customerNumber, SUM(amount) AS totalPay FROM payments GROUP BY customerNumber HAVING totalPay = (SELECT MAX(t.totalAmt) FROM (SELECT SUM(amount) totalAmt FROM payments GROUP BY customerNumber)t);
44. SELECT * FROM employees WHERE officeCode = (SELECT officeCode FROM employees WHERE firstName='Diane' AND lastName='Murphy');
45. SELECT * FROM products WHERE productCode NOT IN (SELECT productCode FROM orderdetails);
46. SELECT DISTINCT customerNumber FROM payments WHERE amount > (SELECT AVG(amount) FROM payments);
47. SELECT productLine, COUNT(*) FROM products GROUP BY productLine HAVING COUNT(*)>5;
48. SELECT c.* FROM customers c LEFT JOIN payments p USING(customerNumber) WHERE p.customerNumber IS NULL;
49. SELECT salesRepEmployeeNumber, COUNT(*) FROM customers GROUP BY salesRepEmployeeNumber HAVING COUNT(*) = (SELECT MAX(cnt) FROM (SELECT COUNT(*) cnt FROM customers GROUP BY salesRepEmployeeNumber)t);
50. SELECT productLine, productName, buyPrice FROM products p WHERE buyPrice = (SELECT MAX(buyPrice) FROM products WHERE productLine=p.productLine);
51. SELECT customerNumber FROM orders o JOIN orderdetails od USING(orderNumber) GROUP BY customerNumber, productCode HAVING COUNT(*)>1;
52. SELECT NOW();
53. SELECT COUNT(*) FROM customers;
54. SELECT customerName, SUBSTRING(customerName,1,5) FROM customers;
55. SELECT * FROM orders WHERE DATEDIFF(NOW(), orderDate)<=30;
56. SELECT orderNumber, DATE_FORMAT(orderDate,'%d-%m-%Y') FROM orders;
57. SELECT customerName, LENGTH(customerName) FROM customers;
58. SELECT employeeNumber, LOWER(lastName) FROM employees;
59. SELECT orderNumber, SUM(quantityOrdered*priceEach) FROM orderdetails GROUP BY orderNumber;
60. SELECT paymentDate, YEAR(paymentDate), MONTH(paymentDate) FROM payments;
